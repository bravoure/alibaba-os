{
  "name": "craftcms/alibaba-oss",
  "description": "Alibaba Cloud OSS integration for Craft CMS",
  "type": "craft-plugin",
  "keywords": [
    "alibaba",
    "cms",
    "craftcms",
    "flysystem",
    "oss",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Pixel & Tonic",
      "homepage": "https://pixelandtonic.com/"
    }
  ],
  "support": {
    "email": "support@craftcms.com",
    "issues": "https://github.com/bravoure/alibaba-oss/issues?state=open",
    "source": "https://github.com/bravoure/alibaba-oss",
    "docs": "https://github.com/bravoure/alibaba-oss/blob/master/README.md",
    "rss": "https://github.com/bravoure/alibaba-oss/commits/master.atom"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0.2",
    "craftcms/cms": "^4.0.0-beta.1",
    "craftcms/flysystem": "^1.0.0-beta.2",
    "aliyuncs/oss-sdk-php": "^2.6",
    "iidestiny/flysystem-oss": "^4"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "craft\\alibabaoss\\": "src/"
    }
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "extra": {
    "name": "Alibaba OSS",
    "handle": "alibaba-oss",
    "documentationUrl": "https://github.com/bravoure/alibaba-oss/blob/master/README.md"
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
